  <item>
    <name>Use Command_L + Arrow (left or right) to switch tmux window</name>
    <identifier>private.command_l_arrow_l_r_to_alt_arrow_l_r</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L,
      KeyCode::CURSOR_RIGHT, ModifierFlag::OPTION_L,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L,
      KeyCode::CURSOR_LEFT, ModifierFlag::OPTION_L,
    </autogen>
  </item>
  <item>
    <name>Use Command_L + (h or l) to switch tmux window</name>
    <identifier>private.command_l_hl_to_alt_l_hl</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyToKey__
      KeyCode::H, ModifierFlag::COMMAND_L,
      KeyCode::H, ModifierFlag::OPTION_L,
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::L, ModifierFlag::COMMAND_L,
      KeyCode::L, ModifierFlag::OPTION_L,
    </autogen>
  </item>
  <item>
    <name>Ctrl-Escape toggles Normal Mode</name>
    <identifier>remap.vimnormal_ctrlbracketleft_toggle2</identifier>
    <not>{{UBIQUITOUS_VIM_BINDINGS_IGNORE_APPS}}</not>
    <autogen>
      __KeyToKey__
      KeyCode::ESCAPE, MODIFIERFLAG_EITHER_LEFT_OR_RIGHT_CONTROL,
      KeyCode::VK_LOCK_ALL_FORCE_OFF,
      KeyCode::VK_CONFIG_TOGGLE_notsave_ubiq_vimnormal,
      {{ UBIQUITOUS_VIM_BINDINGS_CANCEL_OPERATOR_PENDING }}
    </autogen>
  </item>
