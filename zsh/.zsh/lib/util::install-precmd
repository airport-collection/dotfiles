# vim: filetype=zsh sw=2 ts=2 sts=2 et tw=80 foldlevel=0 nospell

declare -ag precmd_functions
for s in "${precmd_functions[@]}"; do
  if [[ "$s" == "util::powerline-shell" || "$s" == "util::copy-tmux-vars" ]]; then
    return
  fi
done
precmd_functions+=(util::powerline-shell util::copy-tmux-vars)
