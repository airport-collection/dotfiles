partial modifier_keys
xkb_symbols "normal" {
    key <AC06> { [ Left, Left ] };
    key <AC07> { [ Down, Down ] };
    key <AC08> { [ Up, Up ] };
    key <AC09> { [ Right, Right ] };
};

partial modifier_keys
xkb_symbols "insert" {
    key <AC06> { [ h, h ] };
    key <AC07> { [ j, j ] };
    key <AC08> { [ k, k ] };
    key <AC09> { [ l, l ] };
};
